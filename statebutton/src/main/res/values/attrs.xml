<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="icon" format="reference" />
    <attr name="iconWidth" format="reference|dimension" />
    <attr name="iconHeight" format="reference|dimension" />
    <attr name="iconSize" format="reference|dimension" />

    <declare-styleable name="StateButton">
        <attr name="icon" />
        <attr name="iconWidth" />
        <attr name="iconHeight" />
        <attr name="supportLoading" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SpringButton">
        <attr name="icon" />
        <attr name="iconWidth" />
        <attr name="iconHeight" />
        <attr name="iconSize" />
        <attr name="android:text" />
        <attr name="android:textSize" />
        <attr name="android:textColor" />
    </declare-styleable>

    <declare-styleable name="MaterialProgressView">
        <attr name="strokeWidth" format="reference|dimension" />
        <attr name="innerRadius" format="reference|dimension" />
        <attr name="size" format="reference|dimension" />
        <attr name="progressBarColor" format="reference|color" />
        <attr name="progressBarColorScheme" format="reference" />
        <attr name="shape" format="enum">
            <enum name="circle" value="0" />
            <enum name="rounded_rectangle" value="1" />
        </attr>
    </declare-styleable>

</resources>